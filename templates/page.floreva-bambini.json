    "custom_liquid_ffBFT9": {
      "type": "custom-liquid",
      "name": "t:sections.custom-liquid.presets.name",
      "settings": {
        "custom_liquid": "<style>\n@import url('https://fonts.googleapis.com/css2?family=Poppins:wght@400;500;600;700&display=swap');\n\n.floreva-science-section {\n  background: #FFFFFF;\n  padding: 50px 20px;\n  font-family: 'Poppins', sans-serif;\n}\n\n.science-container {\n  max-width: 1200px;\n  margin: 0 auto;\n}\n\n.science-heading {\n  text-align: center;\n  font-size: 32px;\n  font-weight: 700;\n  color: #2C4A42;\n  margin-bottom: 50px;\n  font-family: 'Poppins', sans-serif;\n}\n\n.progress-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));\n  gap: 40px;\n  margin-top: 40px;\n}\n\n.progress-item {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  text-align: center;\n}\n\n.circle-wrapper {\n  position: relative;\n  width: 150px;\n  height: 150px;\n  margin-bottom: 20px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.circle-bg {\n  fill: none;\n  stroke: #D1FAE5;\n  stroke-width: 12;\n}\n\n.circle-progress {\n  fill: none;\n  stroke: #10b981;\n  stroke-width: 12;\n  stroke-linecap: round;\n  transform: rotate(-90deg);\n  transform-origin: 50% 50%;\n  transition: stroke-dashoffset 1.5s ease-in-out;\n}\n\n.circle-text {\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  font-size: 36px;\n  font-weight: 700;\n  color: #2C4A42;\n  font-family: 'Poppins', sans-serif;\n  z-index: 10;\n}\n\n.progress-description {\n  font-size: 15px;\n  line-height: 1.6;\n  color: #334155;\n  max-width: 280px;\n  font-family: 'Poppins', sans-serif;\n}\n\n.progress-description strong {\n  color: #10b981;\n  font-weight: 700;\n}\n\n@media (max-width: 768px) {\n  .floreva-science-section {\n    padding: 40px 15px;\n  }\n  \n  .science-heading {\n    font-size: 26px;\n    margin-bottom: 35px;\n  }\n  \n  .progress-grid {\n    grid-template-columns: 1fr;\n    gap: 35px;\n  }\n  \n  .circle-wrapper {\n    width: 140px;\n    height: 140px;\n  }\n  \n  .circle-text {\n    font-size: 32px;\n  }\n  \n  .progress-description {\n    font-size: 14px;\n  }\n}\n\n/* Animation au scroll */\n.progress-item.animate .circle-progress {\n  stroke-dashoffset: attr(data-final-offset);\n}\n</style>\n\n<section class=\"floreva-science-section\">\n  <div class=\"science-container\">\n    \n    <h2 class=\"science-heading\">Cosa Dice la Scienza</h2>\n    \n    <div class=\"progress-grid\">\n      \n      <!-- Circle 1: Energia - 93% -->\n      <div class=\"progress-item\" data-percentage=\"93\">\n        <div class=\"circle-wrapper\">\n          <svg width=\"150\" height=\"150\" viewBox=\"0 0 150 150\">\n            <circle class=\"circle-bg\" cx=\"75\" cy=\"75\" r=\"65\"></circle>\n            <circle class=\"circle-progress\" cx=\"75\" cy=\"75\" r=\"65\" \n                    stroke-dasharray=\"408.4\" \n                    stroke-dashoffset=\"408.4\"\n                    data-final-offset=\"28.6\"></circle>\n          </svg>\n          <div class=\"circle-text\">93%</div>\n        </div>\n        <div class=\"progress-description\">\n          dei genitori ha notato <strong>pi√π energia</strong> e <strong>umore migliore</strong> nel proprio bambino\n        </div>\n      </div>\n      \n      <!-- Circle 2: Digestione - 87% -->\n      <div class=\"progress-item\" data-percentage=\"87\">\n        <div class=\"circle-wrapper\">\n          <svg width=\"150\" height=\"150\" viewBox=\"0 0 150 150\">\n            <circle class=\"circle-bg\" cx=\"75\" cy=\"75\" r=\"65\"></circle>\n            <circle class=\"circle-progress\" cx=\"75\" cy=\"75\" r=\"65\" \n                    stroke-dasharray=\"408.4\" \n                    stroke-dashoffset=\"408.4\"\n                    data-final-offset=\"53.1\"></circle>\n          </svg>\n          <div class=\"circle-text\">87%</div>\n        </div>\n        <div class=\"progress-description\">\n          dei genitori ha riportato una <strong>digestione pi√π regolare</strong> e <strong>abitudini intestinali pi√π facili</strong>\n        </div>\n      </div>\n      \n      <!-- Circle 3: Sonno - 90% -->\n      <div class=\"progress-item\" data-percentage=\"90\">\n        <div class=\"circle-wrapper\">\n          <svg width=\"150\" height=\"150\" viewBox=\"0 0 150 150\">\n            <circle class=\"circle-bg\" cx=\"75\" cy=\"75\" r=\"65\"></circle>\n            <circle class=\"circle-progress\" cx=\"75\" cy=\"75\" r=\"65\" \n                    stroke-dasharray=\"408.4\" \n                    stroke-dashoffset=\"408.4\"\n                    data-final-offset=\"40.8\"></circle>\n          </svg>\n          <div class=\"circle-text\">90%</div>\n        </div>\n        <div class=\"progress-description\">\n          dei genitori ha detto che il proprio bambino <strong>dorme pi√π a lungo</strong> e <strong>pi√π profondamente</strong>\n        </div>\n      </div>\n      \n    </div>\n    \n  </div>\n</section>\n\n<script>\n(function() {\n  const observerOptions = {\n    threshold: 0.3,\n    rootMargin: '0px'\n  };\n\n  const observer = new IntersectionObserver((entries) => {\n    entries.forEach(entry => {\n      if (entry.isIntersecting && !entry.target.classList.contains('animated')) {\n        const progressCircle = entry.target.querySelector('.circle-progress');\n        const finalOffset = progressCircle.getAttribute('data-final-offset');\n        \n        // Animer le cercle\n        setTimeout(() => {\n          progressCircle.style.strokeDashoffset = finalOffset;\n        }, 100);\n        \n        entry.target.classList.add('animated');\n        observer.unobserve(entry.target);\n      }\n    });\n  }, observerOptions);\n\n  document.querySelectorAll('.progress-item').forEach(item => {\n    observer.observe(item);\n  });\n})();\n</script>",
        "color_scheme": "",
        "padding_top": 0,
        "padding_bottom": 0
      }
    },
