{% liquid
  assign heading = section.settings.heading
  assign heading_size = section.settings.heading_size
  assign heading_weight = section.settings.heading_weight
  assign heading_color = section.settings.heading_color
  assign heading_font = section.settings.heading_font

  assign heading_size_mobile = section.settings.heading_size_mobile
  assign heading_weight_mobile = section.settings.heading_weight_mobile

  assign subheading = section.settings.subheading
  assign subheading_size = section.settings.subheading_size
  assign subheading_weight = section.settings.subheading_weight
  assign subheading_color = section.settings.subheading_color
  assign subheading_font = section.settings.subheading_font

  assign subheading_size_mobile = section.settings.subheading_size_mobile
  assign subheading_weight_mobile = section.settings.subheading_weight_mobile

  assign compare_text_color = section.settings.compare_text_color
  assign compare_text_size = section.settings.compare_text_size
  assign compare_text_weight = section.settings.compare_text_weight
  assign compare_text_font = section.settings.compare_text_font
  assign compare_background = section.settings.compare_background
  assign compare_background_active = section.settings.compare_background_active
  assign compare_text_color_active = section.settings.compare_text_color_active

  assign compare_text_size_mobile = section.settings.compare_text_size_mobile
  assign compare_text_weight_mobile = section.settings.compare_text_weight_mobile

  assign compare_radius = section.settings.compare_radius
  assign compare_border_size = section.settings.compare_border_size
  assign compare_border_color = section.settings.compare_border_color
  assign compare_border_style = section.settings.compare_border_style

  assign enable_logo_border = section.settings.enable_logo_border
  assign logo_border_size = section.settings.logo_border_size
  assign logo_border_color = section.settings.logo_border_color
  assign logo_border_style = section.settings.logo_border_style

  assign vs_bg_color = section.settings.vs_bg_color
  assign vs_text_color = section.settings.vs_text_color

  assign line_color = section.settings.line_color
  assign line_size = section.settings.line_size
  assign line_style = section.settings.line_style

  assign bottom_text = section.settings.bottom_text
  assign bottom_text_size = section.settings.bottom_text_size
  assign bottom_text_weight = section.settings.bottom_text_weight
  assign bottom_text_color = section.settings.bottom_text_color
  assign bottom_text_font = section.settings.bottom_text_font

  assign bottom_text_size_mobile = section.settings.bottom_text_size_mobile
  assign bottom_text_weight_mobile = section.settings.bottom_text_weight_mobile

  assign layout_margin_top = section.settings.layout_margin_top
  assign layout_margin_bottom = section.settings.layout_margin_bottom
  assign layout_width = section.settings.layout_width
  %}

<style>
  .section-pc-{{section.id}} {
    border: solid #000000 0px;
    margin-top: {{ layout_margin_top }}px;
    margin-bottom: {{ layout_margin_bottom }}px;
    margin-left: auto;
    margin-right: auto;
    border-radius: 0px;
    overflow: hidden;
    {% if layout_width == true %}
        max-width: 100%;
        width: 100%;
        {% else %}
        max-width: 120rem;
        width: 100%;
      {% endif %}
  }

  .section-pc-{{section.id}}-settings {
    margin: 0 auto;
    padding-top: 54px;
    padding-bottom: 54px;
    padding-left: 1.5rem;
    padding-right: 1.5rem;
  }


  .comparison-heading-pc-{{section.id}} {
    text-align: center;
  }

  .comparison-heading-pc-{{section.id}} * {
    margin: 0;
    font-size: {{ heading_size_mobile }}px;
    color: {{ heading_color }};
    line-height: 130%;
    font-weight: {{ heading_weight_mobile }};
    font-family: {{ heading_font.family }}, {{ heading_font.fallback_families }};
    text-transform: unset;
    text-decoration: none;
  }

  .comparison-text-pc-{{section.id}} {
    margin-top: 16px;
    text-align: center;
  }

  .comparison-text-pc-{{section.id}} * {
    margin: 0;
    font-size: {{ subheading_size_mobile }}px;
    color: {{ subheading_color }};
    font-weight: {{ subheading_weight_mobile }};
    font-family: {{ subheading_font.family }}, {{ subheading_font.fallback_families }};
    line-height: 150%;
    text-transform: unset;
    text-decoration: none;
  }

  .comparison-grid-pc-{{section.id}} {
    display: flex;
  }

  .comparison-table-wrapper-pc-{{section.id}} {
    overflow-x: scroll;
    scrollbar-width: none;
    margin-left: -1.5rem;
    margin-right: -1.5rem;
  }

  .comparison-table-inner-pc-{{section.id}} {
    min-width: 724px;
    padding-left: 1.5rem;
    padding-right: 1.5rem;
  }

  .comparison-table-wrapper-pc-{{section.id}}::-webkit-scrollbar {
    display: none;
  }

  .comparison-table-pc-{{section.id}} {
    padding-bottom: 25px;
    padding-right: 25px;
    margin-top: 12px;
  }

  .comparison-item-pc-{{section.id}} {
    position: relative;
    display: flex !important;
    flex: 1;
    flex-direction: column;
    align-items: center;
    justify-content: center;
  }

  .comparison-item-pc-{{section.id}}:first-child {
    flex-direction: row;
    gap: 20px;
  }

  .comparison-grid-header-pc-{{section.id}} {
    padding-top: 12px;
  }

  .comparison-grid-rows-pc-{{section.id}} {
    border-bottom: {{ line_size }}px {{ line_style }} {{ line_color}};
  }

  .comparison-grid-header-pc-{{section.id}} .comparison-item-pc-{{section.id}} {
    padding: 20px 4px;
  }

  .comparison-grid-header-pc-{{section.id}} .comparison-item-pc-{{section.id}}:not(:first-child),
  .comparison-grid-rows-pc-{{section.id}} .comparison-item-pc-{{section.id}}:not(:first-child) {
    background-color: {{ compare_background }};
  }

  .comparison-grid-rows-pc-{{section.id}}:last-child .comparison-item-pc-{{section.id}}:not(:first-child):after {
    content: '';
    position: absolute;
    width: 100%;
    height: 25px;
    left: 0;
    bottom: calc(-25px - 1px);

    background-color: {{ compare_background }};
  }

  .comparison-grid-rows-pc-{{section.id}}:last-child .comparison-item-pc-{{section.id}}:last-child:after {
    border-bottom-right-radius: {{ compare_radius }}px;
  }

  .comparison-grid-rows-pc-{{section.id}}:last-child .comparison-item-pc-{{section.id}}:nth-child(3):after {
    border-bottom-left-radius: {{ compare_radius }}px;
  }

  .comparison-grid-header-pc-{{section.id}} .comparison-item-pc-{{section.id}}.active {
    background-color: {{ compare_background_active }};
  }

  .comparison-grid-header-pc-{{section.id}} .comparison-item-pc-{{section.id}}.active {
    border: 0px;
  }

  .comparison-grid-header-pc-{{section.id}} .comparison-column-heading-pc-{{section.id}} {
    margin: 16px 0 0 0;
    width: 100%;
    font-size: 14px;
    color: {{ compare_text_color}};
    line-height: 130%;
    text-transform: unset;
    text-align: center;
    text-decoration: none;
  }

  .comparison-grid-header-pc-{{section.id}} .comparison-item-pc-{{section.id}}.active h3 {
    color: {{ compare_text_color_active }};
  }

  .comparison-grid-rows-pc-{{section.id}} .comparison-item-pc-{{section.id}} {
    padding: 20px 4px;
  }

  .comparison-grid-header-pc-{{section.id}} + .comparison-grid-rows-pc-{{section.id}} .comparison-item-pc-{{section.id}}:first-child {
    border-top-left-radius: {{ compare_radius }}px;
  }

  .comparison-grid-header-pc-{{section.id}} .comparison-item-pc-{{section.id}}:nth-child(2) {
    border-top-left-radius: {{ compare_radius }}px;

    border-top-right-radius: {{ compare_radius }}px;

  }

  .comparison-grid-header-pc-{{section.id}} .comparison-item-pc-{{section.id}}:nth-child(3) {
    border-top-left-radius: {{ compare_radius }}px;
  }

  .comparison-grid-header-pc-{{section.id}} .comparison-item-pc-{{section.id}}:last-child:before {
    content: '';
    position: absolute;
    width: 25px;
    height: 100%;
    right: -25px;
    border-top-right-radius: {{ compare_radius }}px;
    background-color: {{ compare_background }};
  }

  .comparison-grid-rows-pc-{{section.id}} .comparison-item-pc-{{section.id}}:last-child:before {
    content: '';
    position: absolute;
    width: 25px;
    height: calc(100% + 1px);
    right: -25px;
    background-color: {{ compare_background}};
  }

  .comparison-grid-rows-pc-{{section.id}}:last-child .comparison-item-pc-{{section.id}}:nth-child(2):after {
    content: '';
    position: absolute;
    width: 100%;
    height: 25px;
    left: 0;
    bottom: calc(-25px - 1px);
    border-bottom-right-radius: {{ compare_radius }}px;
    border-bottom-left-radius: {{ compare_radius }}px;
    background-color: {{ compare_background_active }};
  }

  .comparison-grid-rows-pc-{{section.id}}:last-child .comparison-item-pc-{{section.id}}:last-child:after {
    width: calc(100% + 25px);
    border-bottom-right-radius: {{ compare_radius }}px;
  }

  .comparison-grid-rows-pc-{{section.id}} .comparison-item-pc-{{section.id}}.active {
    background-color: {{ compare_background_active }};
  }

  .comparison-grid-rows-pc-{{section.id}}:last-child .comparison-item-pc-{{section.id}}:first-child:before {
    bottom: 24px
  }

  .comparison-grid-header-pc-{{section.id}} .comparison-item-pc-{{section.id}}:first-child {
    border: 0px;
  }

  .comparison-grid-rows-pc-{{section.id}} .comparison-item-pc-{{section.id}}:first-child h3 {
    display: block;
    margin: 0;
    font-size: {{ compare_text_size_mobile }}px;
    color: {{ compare_color}};
    line-height: 130%;
    text-transform: unset;
    text-align: center;
    font-weight: {{ compare_text_weight_mobile }};
    font-family: {{ compare_text_font.family }}, {{ compare_text_font.fallback_families }};
  }

  .comparison-item-heading-image-pc-{{section.id}} {
    display: block;
    {% if enable_logo_border == true %}
    border: {{logo_border_size}}px {{ logo_border_style }} {{ logo_border_color }};
    {% endif %}
    border-radius: 100px;
    width: 100%;
    max-width: 70px;
    overflow: hidden;
    aspect-ratio: 1/1;
  }

  .comparison-item-heading-image-pc-{{section.id}} img,
  .comparison-item-heading-image-pc-{{section.id}} svg {
    display: block;
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .comparison-item-heading-image-pc-{{section.id}} svg {
    /* background-color: #f10707; */
  }

  .comparison-value-pc-{{section.id}} {
    margin: 0;
    font-size: {{ compare_text_size_mobile }}px;
    color: {{ compare_text_color}};
    line-height: 130%;
    font-family: {{ compare_text_font.family }}, {{ compare_text_font.fallback_families }};
    word-break: break-word;
    text-transform: unset;
    text-align: center;
  }

  .comparison-item-pc-{{section.id}}.active .comparison-value-pc-{{section.id}} {
    color: {{ compare_text_color_active}};
  }

  .comparison-value-icon-pc-{{section.id}} {
    width: 24px;
    height: 24px;
  }

  .comparison-value-icon-pc-{{section.id}} img,
  .comparison-value-icon-pc-{{section.id}} svg {
    display: block;
    object-fit: cover;
    width: 100%;
    height: 100%;
  }

  .comparison-value-icon-pc-{{section.id}} svg path {
    stroke: {{ compare_text_color}};
  }

  .comparison-item-pc-{{section.id}}.active .comparison-value-icon-pc-{{section.id}} svg path {
    stroke: {{ compare_text_color_active}};
  }

  .comparison-versus-icon-pc-{{section.id}} {
    position: absolute;
    top: 50%;
    right: calc(-24px - 5px);
    z-index: 2;
    transform: translateY(-50%);
    overflow: hidden;
    width: 48px;
    height: 48px;
  }

  .comparison-versus-icon-pc-{{section.id}} svg {
    display: block;
    object-fit: cover;
    width: 100%;
    height: 100%;
  }

  .comparison-versus-icon-pc-{{section.id}} svg rect {
    fill: {{ vs_bg_color }};
  }

  .comparison-versus-icon-pc-{{section.id}} svg path {
    fill: {{vs_text_color}};
  }

  .comparison-bottom-text-pc-{{section.id}} {
    margin-top: 20px;
    text-align: left;
  }

  .comparison-bottom-text-pc-{{section.id}} * {
    margin: 0;
    font-size: {{ bottom_text_size_mobile }}px;
    color: {{ bottom_text_color}};
    font-weight: {{ bottom_text_weight_mobile }};
    font-family: {{ bottom_text_font.family }}, {{ bottom_text_font.fallback_families }};
    line-height: 150%;
    text-transform: unset;
    text-decoration: none;
  }

  @media(min-width: 1024px) {

    .section-pc-{{section.id}} {
      margin-top: {{ layout_margin_top }}px;
      margin-bottom: {{ layout_margin_bottom }}px;
      margin-left: auto;
      margin-right: auto;
      border-radius: 0px;
      {% if layout_width == true %}
        max-width: 100%;
        width: 100%;
        {% else %}
        max-width: 120rem;
        width: 100%;
      {% endif %}
    }

    .section-pc-{{section.id}}-settings {
      padding: 0 5rem;
      padding-top: 72px;
      padding-bottom: 72px;
      padding-left: 5rem;
      padding-right: 5rem;
    }

    .comparison-heading-pc-{{section.id}} {
      text-align: center;
    }

    .comparison-heading-pc-{{section.id}} * {
      font-size: {{ heading_size }}px;
      font-weight: {{ heading_weight }};
    }

    .comparison-text-pc-{{section.id}} {
      margin-top: 16px;
      text-align: center;
    }

    .comparison-text-pc-{{section.id}} * {
      font-size: {{ subheading_size }}px;
        font-weight: {{ subheading_weight }};
    }

    .comparison-table-wrapper-pc-{{section.id}} {
      margin-left: 0rem;
      margin-right: 0rem;
    }

    .comparison-table-inner-pc-{{section.id}} {
      padding-left: 0rem;
      padding-right: 0rem;
    }

    .comparison-table-pc-{{section.id}} {
      margin-top: 24px;
    }

    .comparison-item-heading-image-pc-{{section.id}} {
      max-width: 70px;
    }

    .comparison-grid-header-pc-{{section.id}} {
      padding-top: 30px;
    }

    .comparison-grid-header-pc-{{section.id}} .comparison-item-pc-{{section.id}} {
      padding: 24px 16px;
    }

    .comparison-grid-header-pc-{{section.id}} .comparison-column-heading-pc-{{section.id}} {
      text-align: center;
      font-size: {{ compare_text_size }}px;
      font-weight: {{ compare_text_weight }};
    }

    .comparison-grid-rows-pc-{{section.id}} .comparison-item-pc-{{section.id}} {
      padding: 24px 16px;
    }

    .comparison-grid-rows-pc-{{section.id}} .comparison-item-pc-{{section.id}}:first-child h3 {
      font-size: 18px;
    }

    .comparison-value-pc-{{section.id}} {
      font-size: 20px;
    }

    .comparison-value-icon-pc-{{section.id}}{
      width: 28px;
      height: 28px;
    }

    .comparison-versus-icon-pc-{{section.id}} {
      right: calc(-30px - 5px);
      width: 60px;
      height: 60px;
    }

    .comparison-bottom-text-pc-{{section.id}} {
      margin-top: 24px;
      text-align: center;
    }

    .comparison-bottom-text-pc-{{section.id}} * {
      font-size: {{ bottom_text_size }}px;
        font-weight: {{ bottom_text_weight }};
    }
  }
</style>

<style>
  .comparison-grid-pc-{{section.id}} .comparison-item-pc-{{section.id}}.active {
    margin-right: 10px;
  }

  @media(min-width: 1024px) {
    .comparison-grid-pc-{{section.id}} .comparison-item-pc-{{section.id}}.active {
    margin-right: 20px;
    }
  }
</style>

<div
  class="section-pc-{{section.id}} comparison-pc-{{section.id}}"
  style=""
>
  <div class="section-pc-{{section.id}}-settings">
    {% if heading != blank %}
      <div class="comparison-heading-pc-{{section.id}}">
        <p>{{ heading }}</p>
      </div>
    {% endif %}

    {% if subheading != blank %}
      <div class="comparison-text-pc-{{section.id}}">
        <p>{{ subheading }}</p>
      </div>
    {% endif %}

    <div class="comparison-table-wrapper-pc-{{section.id}}">
      <div class="comparison-table-inner-pc-{{section.id}}">
        <div class="comparison-table-pc-{{section.id}}">
          <div class="comparison-grid-pc-{{section.id}} comparison-grid-header-pc-{{section.id}}">
            <div class="comparison-item-pc-{{section.id}}"></div>

            {% for block in section.blocks %}
              {% if block.type == 'compare_header' %}
                <div class="comparison-item-pc-{{section.id}} {% if forloop.index == 1 %} active{% endif %}">
                  <div class="comparison-item-heading-image-pc-{{section.id}}">
                    <img
                      src="{{ block.settings.logo | img_url: '300x' }}"
                      alt="{{ block.settings.heading }}"
                      loading="lazy"
                    >
                  </div>
                  <h3 class="comparison-column-heading-pc-{{section.id}}">{{ block.settings.heading }}</h3>
                  {% if forloop.index == 1 %}
                  <div class="comparison-versus-icon-pc-{{section.id}}">
                    <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 86 83" height="83" width="86">
                    <rect fill="#6C6AB4" rx="41.2756" height="82.5513" width="85.5513"></rect>
