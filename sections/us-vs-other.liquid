                    </svg>
                  </div>
              {% endif %}
                  </div>
              {% endif %}
            {% endfor %}
          </div>
          {% for block in section.blocks %}
            {% if block.type == 'compare_content' %}
              <div class="comparison-grid-pc-{{section.id}} comparison-grid-rows-pc-{{section.id}}">
                <div class="comparison-item-pc-{{section.id}}">
                  <h3>{{ block.settings.heading }}</h3>
                </div>
                
                      <div class="comparison-item-pc-{{section.id}} active">
                        <div class="comparison-value-icon-pc-{{section.id}}">
                          {% if block.settings.active_1 == true %}
                            <svg
                              xmlns="http://www.w3.org/2000/svg"
                              width="31"
                              height="31"
                              viewBox="0 0 31 31"
                              fill="none"
                            >
                              <path d="M3 17.8571L9.66667 25L28.3333 5" stroke="#000000" stroke-width="2" stroke-linecap="round"></path>
                            </svg>
                          {% else %}
                            <svg
                              width="24"
                              height="24"
                              viewBox="0 0 49 49"
                              fill="none"
                              xmlns="http://www.w3.org/2000/svg"
                            >
                              <path d="M36.5 12.5L12.5 36.5" stroke="#000000" stroke-width="3" stroke-linecap="round" stroke-linejoin="round"></path>
                              <path d="M12.5 12.5L36.5 36.5" stroke="#000000" stroke-width="3" stroke-linecap="round" stroke-linejoin="round"></path>
                            </svg>
                          {% endif %}
                        </div>
                      </div>
                      

                {% for item in section.blocks %}
                  {% if item.type == 'compare_header' %}
                    {% if forloop.index == 2 %}
                      <div class="comparison-item-pc-{{section.id}}">
                        <div class="comparison-value-icon-pc-{{section.id}}">
                          {% if block.settings.active_2 == true %}
                            <svg
                              xmlns="http://www.w3.org/2000/svg"
                              width="31"
                              height="31"
                              viewBox="0 0 31 31"
                              fill="none"
                            >
                              <path d="M3 17.8571L9.66667 25L28.3333 5" stroke="#000000" stroke-width="2" stroke-linecap="round"></path>
                            </svg>
                          {% else %}
                            <svg
                              width="24"
                              height="24"
                              viewBox="0 0 49 49"
                              fill="none"
                              xmlns="http://www.w3.org/2000/svg"
                            >
                              <path d="M36.5 12.5L12.5 36.5" stroke="#000000" stroke-width="3" stroke-linecap="round" stroke-linejoin="round"></path>
                              <path d="M12.5 12.5L36.5 36.5" stroke="#000000" stroke-width="3" stroke-linecap="round" stroke-linejoin="round"></path>
                            </svg>
                          {% endif %}
                        </div>
                      </div>
                    {% endif %}
                  {% endif %}
                {% endfor %}
                {% for item in section.blocks %}
                  {% if item.type == 'compare_header' %}
                    {% if forloop.index == 3 %}
                      <div class="comparison-item-pc-{{section.id}}">
                        <div class="comparison-value-icon-pc-{{section.id}}">
                          {% if block.settings.active_3 == true %}
                            <svg
                              xmlns="http://www.w3.org/2000/svg"
                              width="31"
                              height="31"
                              viewBox="0 0 31 31"
                              fill="none"
                            >
                              <path d="M3 17.8571L9.66667 25L28.3333 5" stroke="#000000" stroke-width="2" stroke-linecap="round"></path>
                            </svg>
                          {% else %}
                            <svg
                              width="24"
                              height="24"
                              viewBox="0 0 49 49"
                              fill="none"
                              xmlns="http://www.w3.org/2000/svg"
